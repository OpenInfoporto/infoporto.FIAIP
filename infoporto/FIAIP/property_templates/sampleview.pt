<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="infoporto.FIAIP">
<body>

<metal:main fill-slot="content-core">
  <metal:content-core define-macro="content-core">



    <div class="property field-full_description" tal:content="view/full_description" />

    <div class="property details-block">
      <div class="details-head">Dettagli</div>

      <div class="details-field">
        <div class="details-label">Rif.</div>
        <div class="details-value" tal:content="item/rif" />
      </div>

    </div>

    <div tal:content="context/Title">Title inserted here</div>

  </metal:content-core>
</metal:main>

</body>
</html>

